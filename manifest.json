{
  "name": "Geeks for Geeks Automation Extension",
  "version": "2.0.0",
  "homepage_url": "https://myperfectice-automation.vercel.app/offer?ref=gfg",
  "description": "Attempt all quiz and articles on Geeks for Geeks automatically",
  "author": "Tanay",
  "manifest_version": 3,
  "icons" : {
    "16": "./images/icon16.png",
    "32": "./images/icon32.png",
    "48": "./images/icon48.png",
    "128": "./images/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "./images/icon16.png",
      "32": "./images/icon32.png",
      "48": "./images/icon48.png",
      "128": "./images/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["scripting", "activeTab", "storage", "tabs"],
  "host_permissions": [
    "https://www.geeksforgeeks.org/*",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.geeksforgeeks.org/*"],
      "js": ["content.js"]
    }
  ]
}